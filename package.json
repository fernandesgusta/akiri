    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "electron": "^33.2.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "svelte": "^5.1.3",
    "svelte-check": "^4.0.5",
    "tslib": "^2.8.0",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "watch": "^1.0.2"
  }
}
